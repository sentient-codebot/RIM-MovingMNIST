{
    "configurations": [
        {
            "name": "visualize logged tensor",
            "type": "python",
            "request": "launch",
            "program": "./visualize_logtensor.py",
            "console": "integratedTerminal",
            "args": [
                "--folder_log",
                "logs/RIM_100_6_MMNIST_complete_0.0007_num_inp_heads_1_ver_0/intermediate_vars"
            ]
        },
        {
            "name": "minimal: only for debugging code",
            "type": "python",
