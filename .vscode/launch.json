            "console": "integratedTerminal",
            "args": [
                "--cfg_json",
                "configs/rim_minimal/rim_minimal.json",
                "--experiment_name",
                "MMNIST_minimal",
                "--loss_fn",
                "mseloss"
            ]
        },
